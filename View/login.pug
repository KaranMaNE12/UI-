    div.container
        div.content
            div.header
                div.logo
            div#logincontainer 
                <div class="login-page">
                <div class="form">
                    <form class="register-form">
                    h2 Personal Registration
                    <input type="text" id='username' placeholder="name" maxlength="50"/>
                    <input type="email" id='usermail' placeholder="email address" maxlength="50"/>
                    <input type="date" id='userbdate' placeholder="Birth Date"/>
                    <input type="number" id='usermobile' placeholder="Mobile Number" maxlength="10" oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);">
                    <input type="password" id='userpass' placeholder="password" maxlength="16"/>
                    <input type="password" id='userconfirm' placeholder="Confirm Password" maxlength="16"/>
                    <button onclick='registeruser()'>create</button>
                    <p class="message">Already registered? <a href="#">Sign In</a></p>
                    </form>
                    <form class="login-form">
                    h2 Login
                    select#logintype(name="login type")
                        option(value="Select") Login Type  
                        option(value="Hospital") Hospital
                        option(value="Hospitalstaff") Hospital Staff
                        option(value="patient") Personal

                    <input type="text" placeholder="Mobile NO" maxlength='10' id='logid'/>
                    <input type="password" placeholder="password" id="logpass" maxlength="16"/>
                    <button onclick="login()">login</button>
                    <p class="message">Not registered? <a href="#">Create an account personal</a></p>
                    span(onclick="hospitalregister()") Create an account hospital
                    </form>
                </div>
                </div>

            div.model12#hospitalregistartion 
                <div class="login-page">
                <div class="form">                    
                    h2 Hospital Registration
                    <input type="text" id='username2' placeholder="User Name" maxlength="50"/>
                    <input type="text" id='hospitalname' placeholder="Hospital Name" maxlength="50"/>
                    <input type="email" id='hemail' placeholder="Email address" maxlength="50"/>
                    <input type="number" id="hmobileno" placeholder="Mobile Number" maxlength="10" oninput="javascript: if (this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);">
                    <input type="date" id="startedon" placeholder="Started On"/>
                    <input type="text" id="hrnnumber" placeholder="HRN Number" maxlength="20">                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
                    <input type="password" id="hpassword" placeholder="Password" maxlength="16"/>
                    <input type="password" id="hcomfirmpass" placeholder="Confirm Password" maxlength="16"/>
                    <button onclick="registerhospital()">create</button>
                    button(onclick="loginpageback()" style="background-color:red;") Back
                
